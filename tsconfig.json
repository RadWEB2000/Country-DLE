{
  "compilerOptions": {
    "target": "ES2017",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@/app/*": [
        "./src/app/*"
      ],
      "@/parts/*": [
        "./src/components/*"
      ],
      "@/data/*": [
        "./src/data/*"
      ],
      "@/lib/*": [
        "./src/lib/*"
      ],
      "@/css/*": [
        "./src/styles/*"
      ],
      "@/ts/*": [
        "./src/types/*"
      ],
      "@/context/*": [
        "./src/contexts/*"
      ],
      "@/provider/*": [
        "./src/providers/*"
      ],
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}